    "@11ty/eleventy-plugin-syntaxhighlight": "^4.2.0",
    "@octokit/plugin-retry": "^4.0.3",
    "@octokit/rest": "^19.0.5",
    "@types/semver": "^7.3.13",
    "eslint": "^8.28.0",
    "eslint-config-planet": "^20.0.3",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.5",
    "semver": "^7.3.8",
    "typescript": "^4.9.3"
  },
  "eslintConfig": {
    "extends": "planet",
